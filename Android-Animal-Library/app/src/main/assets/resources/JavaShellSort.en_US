@import=resources/GenericShellSort.en_US
theName=Shell Sort [EN]
codeExample=public void shellSort(int[] array) {\n  int i, j, dist, v;\n  for (dist = 1; dist < array.length / 9; dist = dist * 3 +1) ;\n  for (; dist > 0; dist /= 3) {\n    for (i = dist; i < array.length; i++) {\n      v = array[i];\n     j = i;\n      while (j >= dist && v < array[j - dist]) {\n        array[j] = array[j-dist]\n        j = j - dist;\n      }\n     array[j] = v;\n   }\n  }\n}
